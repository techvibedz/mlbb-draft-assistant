{
  "version": 2,
  "buildCommand": "pnpm install && pnpm run build",
  "outputDirectory": "out",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "functions": {
    "api/**/*.py": {
      "runtime": "@vercel/python@2.0.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ]
} 